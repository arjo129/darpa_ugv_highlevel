<launch>
    <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
        <rosparam file="$(find simple_obstacle_avoidance)/config/move_base.yml" command="load" /> 
        <remap from="cmd_vel" to="/X1/cmd_vel"/>
    </node>
    <node pkg="localization_manager" type="odom_repub" respawn="false" name="odom_repub" output="screen"/>
    <node pkg="simple_obstacle_avoidance" type="frontier_exploration" name="frontier_exploration" output="screen" respawn="true">
        <remap from="scan" to="/X1/front_scan"/>
    </node>
</launch>