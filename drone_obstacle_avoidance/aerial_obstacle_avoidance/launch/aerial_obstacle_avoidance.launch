		<!-- maximum range to integrate (speedup!) -->
		<param name="sensor_model/max_range" value="10.0" />
		<!-- data source to integrate (PointCloud2) -->
		<remap from="cloud_in" to="/$(arg robot_name)/cloud_in" />
	</node>
	<node pkg="aerial_obstacle_avoidance" type="apf_planner" name="apf_planner" output="screen">
		<param name="cmd_vel_topic_" value="/$(arg robot_name)/cmd_vel" />
		<param name="base_link_" value="/$(arg robot_name)/base_link" />
		<param name="goal_sub_topic_" value="/$(arg robot_name)/goal_to_explore" />
		<param name="obs_sub_topic_" value="/octomap_full" />
		<param name="rate" value="10" />
    <param name="distance_threshold" value="1" />
	</node>
	
</launch>